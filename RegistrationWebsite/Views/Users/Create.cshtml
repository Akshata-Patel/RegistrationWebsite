@model RegistrationWebsite.Models.User
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="app-page-title">
    <div class="page-title-wrapper">
        <div class="page-title-heading">
            <div class="page-title-icon">
                <i class="pe-7s-drawer icon-gradient bg-happy-itmeo">
                </i>
            </div>
            <div>
                New User
                <div class="page-title-subheading">
                </div>
            </div>
        </div>
    </div>
</div>
<div class="main-card mb-3 card">
    <div class="card-body">
        <h5 class="card-title">
        </h5>
        <form method="post" class="needs-validation" novalidate>
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-row">
                <div class="col-md-6">
                    <div class="form-group bmd-form-group">
                        <label asp-for="UserName" class="control-label"></label>
                        <input asp-for="UserName" class="form-control" />
                        <span asp-validation-for="UserName" class="text-danger"></span>
                        <div class="valid-feedback">
                            Looks good!
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group bmd-form-group">
                        <label asp-for="Password" class="control-label">Password</label>
                        <input type="password" asp-for="Password" class="form-control"/>
                        <span asp-validation-for="Password" class="text-danger"></span>
                        <div class="valid-feedback">
                            Looks good!
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-row">
                <div class="col-md-4">
                    <div class="form-group bmd-form-group">
                        <label asp-for="FirstName">First Name</label>
                        <input asp-for="FirstName" class="form-control" placeholder="first name" required />
                        <span asp-validation-for="FirstName" class="text-danger"></span>
                        <div class="valid-feedback">
                            Looks good!
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group bmd-form-group">
                        <label asp-for="LastName">Last Name</label>
                        <input asp-for="LastName" class="form-control" placeholder="last name" required />
                        <span asp-validation-for="LastName" class="text-danger"></span>
                        <div class="valid-feedback">
                            Looks good!
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group bmd-form-group">
                        <label asp-for="DisplayName">Display Name</label>
                        <input asp-for="DisplayName" class="form-control" placeholder="display name" required />
                        <span asp-validation-for="DisplayName" class="text-danger"></span>
                        <div class="valid-feedback">
                            Looks good!
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-row">
                <div class="col-md-6">
                    <div class="form-group bmd-form-group">
                        <label asp-for="Email">Email</label>
                        <input asp-for="Email" class="form-control" placeholder="email" required />
                        <span asp-validation-for="Email" class="text-danger"></span>
                        <div class="valid-feedback">
                            Looks good!
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group bmd-form-group">
                        <label asp-for="MobileNo">Mobile No.</label>
                        <input asp-for="MobileNo" class="form-control" placeholder="mobile no." required />
                        <span asp-validation-for="MobileNo" class="text-danger"></span>
                        <div class="valid-feedback">
                            Looks good!
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-row">
                <div class="col-md-4">
                    <div class="form-group bmd-form-group">
                        <label asp-for="City">City</label>
                        <input asp-for="City" class="form-control" placeholder="city" required />
                        <span asp-validation-for="City" class="text-danger"></span>
                        <div class="valid-feedback">
                            Looks good!
                        </div>
                    </div>
                </div>
                <div class="form-row">
                    <div class="col-md-4">
                        <div class="form-group bmd-form-group">
                            <label asp-for="State">State</label>
                            <input asp-for="State" class="form-control" placeholder="state" required />
                            <span asp-validation-for="State" class="text-danger"></span>
                            <div class="valid-feedback">
                                Looks good!
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="form-group bmd-form-group">
                            <label asp-for="Country">Country</label>
                            <input asp-for="Country" class="form-control" placeholder="country" required />
                            <span asp-validation-for="Country" class="text-danger"></span>
                            <div class="valid-feedback">
                                Looks good!
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="form-group bmd-form-group">
                            <label asp-for="PinCode">Pin Code</label>
                            <input asp-for="PinCode" class="form-control" placeholder="pin code" required />
                            <span asp-validation-for="PinCode" class="text-danger"></span>
                            <div class="valid-feedback">
                                Looks good!
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-row">
                <div class="col-md-12">
                    <div class="form-group bmd-form-group">
                        <label asp-for="Address">Address</label>
                        <input asp-for="Address" class="form-control" placeholder="address" value="" required />
                        <span asp-validation-for="Address" class="text-danger"></span>
                        <div class="valid-feedback">
                            Looks good!
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-row">
                <div class="col-md-12">
                    <div class="form-group bmd-form-group">
                        <label asp-for="ShippingAddress">Shipping Address</label>
                        <input asp-for="ShippingAddress" class="form-control" placeholder="shipping address" value="" required />
                        <span asp-validation-for="ShippingAddress" class="text-danger"></span>
                        <div class="valid-feedback">
                            Looks good!
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
            <div class="clearfix"></div>
            <div>
                <a asp-action="Index">Back to List</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}


